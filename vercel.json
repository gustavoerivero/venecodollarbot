{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/index.ts"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ],
  "crons": [{
    "path": "/api",
    "schedule": "* 9,13 * * 1-5"
  }]
}