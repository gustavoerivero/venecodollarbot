{
  "version": 2,
  "public": true,
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "api/index.ts"
    }
  ],
  "rewrites": [
    {
      "source": "/api/cron",
      "destination": "api/cron/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "api/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 13 * * 1-5"
    },
    {
      "path": "/api/cron",
      "schedule": "0 17 * * 1-5"
    }
  ]
}